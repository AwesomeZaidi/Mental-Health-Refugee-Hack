{
  "name": "health popup",
  "manifest_version": 2,
  "version": "1.0",
  "description": "A depression screening extension!",
  "permissions": ["declarativeContent","storage"],
  "background": {
    "scripts": ["background.js"]
  },
  "page_action": {
    "default_title": "Your daily check in...",
    "default_icon": "icon48.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "icon48.png"
  }
}
